{
  "intents": [
    {
      "tag": "greeting",

      "patterns": [
        "Hi",
        "How are you",
        "Is anyone there?",
        "Hello",
        "Good day",
        "Whats up"
      ],
      "responses": [
        "Hello!",
        "Good to see you again!",
        "Hi there, how can I help?"
      ]
    },
    {
      "tag": "goodbye",
      "patterns": ["Bye", "See you later", "Goodbye"],
      "responses": ["See you later", "Have a great day", "Goodbye!"]
    },
    {
      "tag": "thanks",
      "patterns": [
        "Thanks",
        "Thank you",
        "That's helpful",
        "Thanks for the help"
      ],
      "responses": ["You're welcome", "Anytime!", "My pleasure"]
    },
    {
      "tag": "noanswer",
      "patterns": [],
      "responses": [
        "Sorry, can't understand you",
        "Please give me more info",
        "Not sure I understand"
      ]
    },
    {
      "tag": "options",
      "patterns": [
        "How you could help me?",
        "What you can do?",
        "What help you provide?",
        "How you can be helpful?",
        "What support is offered"
      ],
      "responses": [
        "I can help you with appointment, services, location, hours, contact, price, payment, shop, advice, name, age"
      ]
    },
    {
      "tag": "advice",
      "patterns": [
        "Can you help me?",
        "What should I do?",
        "I need a advice",
        "What you suggest?",
        "I need help"
      ],
      "responses": ["Sure, I can help you. Just tell me what you need"]
    },
    {
      "tag": "name",
      "patterns": ["What is your name?", "Do you have a name?", "Name"],
      "responses": ["I am a bot. I don't have a name"]
    },
    {
      "tag": "age",
      "patterns": ["How old are you?", "What is your age?", "Age"],
      "responses": ["I am a bot. I don't have an age"]
    },
    {
      "tag": "shop",
      "patterns": [
        "Where is the shop?",
        "Where can I buy?",
        "Where is the store?",
        "Where can I shop?"
      ],
      "responses": ["You can shop online at our website"]
    },
    {
      "tag": "hours",
      "patterns": [
        "What are your hours?",
        "When are you open?",
        "What are your hours of operation?"
      ],
      "responses": ["We are open 7 days a week from 9am to 5pm"]
    },
    {
      "tag": "location",
      "patterns": [
        "Where are you located?",
        "Where is your location?",
        "What is your address?"
      ],
      "responses": ["We are located at 123 Main St, Miami, FL 33101"]
    },
    {
      "tag": "appointment",
      "patterns": [
        "How do I make an appointment?",
        "How do I schedule an appointment?",
        "Can I make an appointment?"
      ],
      "responses": ["To make an appointment, please call us at 123-456-7890"]
    },
    {
      "tag": "contact",
      "patterns": [
        "How can I contact you?",
        "What is your phone number?",
        "What is your email?"
      ],
      "responses": [
        "You can contact us by phone at 123-456-7890 or by email at asd@asd.com"
      ]
    },
    {
      "tag": "services",
      "patterns": [
        "What services do you offer?",
        "What do you offer?",
        "What services are available?"
      ],
      "responses": [
        "We offer a variety of services including haircuts, color, styling, and more"
      ]
    },
    {
      "tag": "price",
      "patterns": [
        "How much does it cost?",
        "What is the price?",
        "What are your prices?"
      ],
      "responses": [
        "Our prices vary depending on the service. Please contact us for more information"
      ]
    },
    {
      "tag": "payment",
      "patterns": [
        "What payment methods do you accept?",
        "Do you accept credit cards?",
        "What are your payment options?"
      ],
      "responses": ["We accept cash, credit cards, and mobile payments"]
    }
  ]
}
